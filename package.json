{
  "name": "plot-data",
  "version": "0.8.5",
  "description": "",
  "main": "./lib/core.js",
  "type": "module",
  "scripts": {
    "dev": "webpack --config webpack-dev.config.cjs",
    "build": "webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "start": "http-server -a localhost -p 3030",
    "cy:run-base": "cypress run --env type=base",
    "cy:run-actual": "cypress run --env type=actual",
    "cy:run-open": "cypress open --env type=actual",
    "test": "npm run dev & start-server-and-test start http-get://localhost:3030 cy:run-actual",
    "base-test": "npm run dev & start-server-and-test start http-get://localhost:3030 cy:run-base",
    "run-cyopen": "npm run dev & start-server-and-test start http-get://localhost:3030 cy:run-open"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dessia-tech/plot_data.git"
  },
  "keywords": [
    "dessia",
    "plot-data"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dessia-tech/plot_data/issues"
  },
  "homepage": "http://dessia.tech/",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/webpack-preprocessor": "^5.16.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.17.23",
    "browserify": "^17.0.0",
    "cypress": "^12.4.1",
    "cypress-visual-regression": "^2.1.0",
    "jasmine": "^4.5.0",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.15.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "commitlint": "^13.2.1",
    "fast-deep-equal": "^3.1.3",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^5.5.0",
    "ignore-errors": "^2.0.0",
    "ts-loader": "^9.2.6",
    "webpack": "^5.60.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
